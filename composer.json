{
  "name": "clearcode/simple-bus-elk-bundle",
  "description": "ELK integration for Symfony2",
  "type": "bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Clearcode",
      "homepage": "http://clearcode.cc/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "psr/log": "^1.0",
    "symfony/framework-bundle": "~2.5",
    "symfony/monolog-bundle": "~2.4",
    "simple-bus/message-bus": "~2.0",
    "jms/serializer-bundle": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Clearcode\\SimpleBusElkBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4" : { "tests\\Clearcode\\SimpleBusElkBundle\\" : "tests" }
  },
  "require-dev": {
    "phpunit/phpunit" : "~5.0",
    "satooshi/php-coveralls": "^0.7.1"
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
